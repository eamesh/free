{
  "name": "free-core",
  "version": "1.1.6-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/eamesh/free.git"
  },
  "files": [
    "lib",
    "node_modules",
    "package.json",
    "README.md"
  ],
  "main": "lib/umd/index.js",
  "module": "lib/es/index.js",
  "style": "lib/style.css",
  "typings": "lib/types/index.d.ts",
  "author": "eamesh",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "rimraf lib && node ./scripts/build-lib.js && pnpm build:dts && rimraf tsconfig.node.tsbuildinfo",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "build:dts": "vue-tsc -b --force tsconfig.esm.json",
    "clean": "rimraf lib",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/estree": "^0.0.51",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vicons/fluent": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "14",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "naive-ui": "^2.27.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.11",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vfonts": "^0.0.3",
    "vite": "^2.9.0",
    "vite-plugin-dts": "^1.0.5",
    "vue-tsc": "^0.29.8",
    "vuedraggable": "^4.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,md}": "pnpm lint:eslint",
    "*.{vue,scss,css,sass}": "pnpm lint:stylelint"
  }
}
